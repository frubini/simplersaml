{
    "name": "ubu-developers/simplersaml",
    "description": "Allows Simpler integration of SimpleSAML and Laravel.",
    "keywords": [ "laravel", "illuminate", "simplesaml", "saml" ],
    "license": "MIT",
    "version": "v1.18.4.2",
    "authors": [
        {
            "name": "Federico Rubini",
            "email": "f.r.rubini@uu.nl"
        }
    ],
    "require": {
        "php": "^7.3",
        "illuminate/support": "^7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "SimplerSaml\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SimplerSaml\\SimplerSamlServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable"
}
